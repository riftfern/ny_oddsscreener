{
  "installCommand": "pnpm install",
  "buildCommand": "pwd && ls -la && pnpm run vercel-build && ls -la apps/web/",
  "outputDirectory": "apps/web/dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
