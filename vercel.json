{
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run vercel-build && ls -la && ls -la apps/web/ && mv apps/web/dist dist",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
